fun main() {
    
    val age = 5
    val name = "Rover"
    
    println("Happy Birthday, ${name}!")
// Let's print a cake!
    println("   ,,,,,   ")
    println("   |||||   ")
    println(" =========")
    println("@@@@@@@@@@@")
    println("{~@~@~@~@~}")
    println("@@@@@@@@@@@")
// This prints an empty line.
    println("")
    println("You are already ${age}, ${name}!")
    println("${age} is the very best age to celebrate!")
}
